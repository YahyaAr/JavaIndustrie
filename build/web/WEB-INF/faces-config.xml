<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <managed-bean>  
    <managed-bean-name>uploadInstance</managed-bean-name>  
    <managed-bean-class>controller.TraitementInstance</managed-bean-class>  
    <managed-bean-scope>session</managed-bean-scope>  
    </managed-bean> 
    <navigation-rule> 
        <from-view-id>/index.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>uploadOK</from-outcome>
        <to-view-id>faces/view/after.xhtml</to-view-id>
    </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <from-view-id>/view/after.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>index</from-outcome>
        <to-view-id>faces/index.xhtml</to-view-id>
    </navigation-case>
    </navigation-rule>
</faces-config>
